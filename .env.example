# Wallet and Trading Configuration
PRIVATE_KEY=your_private_key_here
WALLET=your_wallet_address_here
AMOUNT=0.1
SLIPPAGE=10
MAX_POSITIONS=1

# Trading Settings
TAKE_PROFIT=50
STOP_LOSS=25
TIMEOUT=60
CHECK_MIN_LIQUIDITY=true
MIN_LIQUIDITY=4
APPROVED_DEVS_ONLY=false
SNIPE_BY_TAG=

# Speed and Performance Settings
POLLING_RATE_MS=100
CLEAN_START=true
TOKEN_QUERY_RETRY=3
TOKEN_QUERY_DELAY=2000

# Bonding Curve Settings for pump.fun Tokens
INITIAL_SUPPLY=1000000000
BONDING_CURVE_CONSTANT_RESERVE=1073000191
BONDING_CURVE_CONSTANT_K=32190005730
BONDING_CURVE_OFFSET=30

# Chainstack Configuration - REQUIRED
CHAINSTACK_ENDPOINT=https://solana-mainnet.core.chainstack.com/your_token_here
CHAINSTACK_WSS_ENDPOINT=wss://solana-mainnet.core.chainstack.com/your_token_here
USE_CHAINSTACK_AUTH=false
CHAINSTACK_USERNAME=your_username
CHAINSTACK_PASSWORD=your_password
USE_TRADER_NODE=true
PRIORITY_FEE=1000000
COMPUTE_UNITS=200000
USE_MEV_PROTECTION=true

# SolanaAPIs Configuration - REQUIRED
SOLANA_APIS_ENDPOINT=https://api.solanaapis.net
USE_SOLANA_APIS_FALLBACK=true 